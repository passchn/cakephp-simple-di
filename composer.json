{
    "name": "passchn/cakephp-simple-di",
    "description": "Simple Dependency Injection plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "cakephp/cakephp": "^4.4 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Passchn\\SimpleDI\\": "src/",
            "Passchn\\SimpleDI\\Tests\\Stub\\": "tests/Stub/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestApp\\": "tests/test_app/src/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    }
}
